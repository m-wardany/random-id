{
  "name": "m-wardany/hash-ids",
  "description": "This package is designed to hash sequential keys so that they appear as random characters, making them difficult to predict. There is no requirement to check if the key already exists in the database before saving, as each key is guaranteed to be unique",
  "keywords": [
    "laravel",
    "laravel-packages",
    "youtube-like-id",
    "hashing",
    "encrypted-ids",
    "php-ffx",
    "ffx",
    "fpe",
    "encryption"
  ],
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "MWardany\\HashIds\\": "src/"
    }
  },
  "authors": [
    {
      "name": "Muhammad Wardany",
      "email": "muhammad.wardany@gmail.com"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": "^8.1",
    "katoni/php-ffx": "^1.0"
  },
  "require-dev": {
    "orchestra/testbench": "^9.0",
    "phpunit/phpunit": "^11.0",
    "mockery/mockery": "^1.6"
  },
  "autoload-dev": {
    "psr-4": {
      "MWardany\\HashIds\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit",
    "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
  },
  "extra": {
    "laravel": {
      "providers": [
        "MWardany\\HashIds\\Providers\\ServiceProvider"
      ]
    }
  }
}
